@using Xbim.Ifc4.Interfaces;
@using Xbim.Ifc4.Kernel;
@model List<IIfcPropertySet>

@{
    if (Model != null)
    {
        <table>
            <tbody>
                @foreach (var propSet in Model)
                {
                <tr>
                    <th colspan="2">@propSet.Name</th>
                </tr>
                foreach (IIfcPropertySingleValue prop in propSet.HasProperties)
                {
                <tr title="">
                    <td>@prop.Name</td>
                    <td>@prop.NominalValue</td>
                </tr>
                }
                }
            </tbody>
        </table>
  


    }
} 
