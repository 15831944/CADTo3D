@using Xbim.Ifc2x3.Kernel;
@model List<Xbim.Ifc2x3.Kernel.IfcPropertySet>

@{
    if (Model != null)
    {
        <table>
            <tbody>
                @foreach (var propSet in Model)
                {
                    <tr>
                        <th colspan="2">@propSet.Name</th>
                    </tr>
                    foreach (Xbim.Ifc2x3.PropertyResource.IfcPropertySingleValue prop in propSet.HasProperties)
                    {
                        <tr title="">
                            <td>@prop.Name</td>
                            <td>@prop.NominalValue</td>
                        </tr>
                    }
                }
            </tbody>
        </table>



    }
}

